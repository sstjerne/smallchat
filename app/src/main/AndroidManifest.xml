<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="chat.small.demo.org.smallchat">

    <application
        android:name="chat.small.demo.org.smallchat.BotApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="chat.small.demo.org.smallchat.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name="chat.small.demo.org.smallchat.BotService"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:enabled="true"
            />

    </application>



</manifest>